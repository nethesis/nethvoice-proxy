{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "remove-route input",
  "$id": "http://schema.nethserver.org/nethvoice-proxy/remove-route-input.json",
  "description": "Remove a VoIP route",
  "examples": [
        {
      "domain": "voice.nethserver.org"
    }
  ],
  "type": "object",
